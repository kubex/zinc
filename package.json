{
  "name": "@kubex/zinc",
  "version": "1.0.0",
  "description": "A collection of web components for building web applications based off of @shoelace-style/Shoelace",
  "keywords": [
    "web components",
    "custom elements",
    "lit-element",
    "components"
  ],
  "bugs": {
    "url": "https://github.com/kubex/zinc/issues"
  },
  "repository": {
    "type": "git",
    "directory": "git@github.com:kubex/zinc"
  },
  "license": "MIT",
  "author": "",
  "contributors": [
